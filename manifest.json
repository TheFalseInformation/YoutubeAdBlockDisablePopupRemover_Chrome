{
    "manifest_version": 2,
    "name": "YouTube Popup Remover",
    "version": "0.1",
    "description": "Turns off the 'Turn off your ADBlocker' popup in YouTube.",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://youtube.com/*",
                "https://www.youtube.com/*"
            ],
            "js": [
                "ext.js"
            ]
        }
    ]
}