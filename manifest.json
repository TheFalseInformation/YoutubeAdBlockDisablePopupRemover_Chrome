{
    "manifest_version": 3,
    "name": "YouTube Popup Remover",
    "version": "0.2.2",
    "description": "Turns off the 'Turn off your ADBlocker' popup in YouTube.",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*",
                "https://www.youtube.com/*"
            ],
            "js": [
                "ext.js"
            ]
        }
    ]
}
